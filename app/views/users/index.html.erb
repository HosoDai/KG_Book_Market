<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<table>
  <thead>
    <tr>
      <th>image</th>
      <th>user_name</th>
      <th>faculty</th>
      <th colspan="4"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
    <% @users.each do |user| %>
      <td>image</td>
      <td><%= user.name %></td>
      <td><%= user.faculty %></td>
      <td><%= link_to "Show", user_path(user.id) %></td>
    </tr>
  <% end %>
  </tbody>
</table>

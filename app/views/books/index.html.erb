<h1>Books#index</h1>
<p>Find me in app/views/books/index.html.erb</p>

<table>
  <thead>
    <tr>
      <th>image</th>
      <th>title</th>
      <th>user_name</th>
      <th colspan="4"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
    <% @books.each do |book| %>
      <td>image</td>
      <td><%= link_to book.title, book_path(book.id) %></td>
      <td><%= book.user.name %></td>
    </tr>
  <% end %>
  </tbody>
</table>
